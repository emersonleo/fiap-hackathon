        {
          "name": "Registrar Morador",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "const pad = (n, w) => String(n).padStart(w, '0');",
                  "const now = Date.now();",
                  "const suffix = pad(now % 1000000, 6);",
                  "const randInt = (min, max) => Math.floor(Math.random() * (max - min + 1)) + min;",
                  "const calcCpfDigit = (base, factor) => {",
                  "  let total = 0;",
                  "  for (let i = 0; i < base.length; i++) {",
                  "    total += parseInt(base.charAt(i), 10) * (factor - i);",
                  "  }",
                  "  const mod = total % 11;",
                  "  return (mod < 2) ? 0 : (11 - mod);",
                  "};",
                  "const genCpf = () => {",
                  "  // gera 9 d√≠gitos base e calcula os 2 verificadores (CPF v√°lido)",
                  "  let base = '';",
                  "  for (let i = 0; i < 9; i++) base += String(randInt(0, 9));",
                  "  // evita CPFs com todos d√≠gitos iguais (ex: 000000000)",
                  "  if (/^(\\d)\\1{8}$/.test(base)) base = '123456789';",
                  "  const d1 = calcCpfDigit(base, 10);",
                  "  const d2 = calcCpfDigit(base + String(d1), 11);",
                  "  return base + String(d1) + String(d2);",
                  "};",
                  "const cpf = genCpf();",
                  "const telefone = `(11) 9${suffix.slice(0, 4)}-${suffix.slice(4, 8).padEnd(4, '0')}`;",
                  "const email = `morador_${suffix}@teste.com`;",
                  "pm.environment.set('register_cpf', cpf);",
                  "pm.environment.set('register_telefone', telefone);",
                  "pm.environment.set('register_email', email);",
                  "pm.environment.set('register_senha', 'Senha@123456');"
                ]
              }
            }
          ],
          "request": {
