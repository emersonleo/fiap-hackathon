      "name": "‚öôÔ∏è Setup",
      "item": [
        {
          "name": "Setup - Carregar vari√°veis din√¢micas",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const baseUrl = pm.environment.get('base_url') || pm.collectionVariables.get('base_url');",
                  "if (!baseUrl) {",
                  "  throw new Error('base_url n√£o definido no ambiente/collection');",
                  "}",
                  "",
                  "const setIfPresent = (key, value) => {",
                  "  if (value !== undefined && value !== null && value !== '') pm.environment.set(key, value);",
                  "};",
                  "",
                  "const safeJson = (res) => {",
                  "  try { return res.json(); } catch (e) { return null; }",
                  "};",
                  "",
                  "const requestJson = (opt) => new Promise((resolve) => {",
                  "  pm.sendRequest(opt, (err, res) => resolve({ err, res }));",
                  "});",
                  "",
                  "(async () => {",
                  "  // 1) Descobrir UBS (p√∫blica)",
                  "  const ubsResp = await requestJson({ url: `${baseUrl}/api/ubs`, method: 'GET' });",
                  "  if (!ubsResp.err && ubsResp.res && ubsResp.res.code === 200) {",
                  "    const data = safeJson(ubsResp.res);",
                  "    if (Array.isArray(data) && data.length > 0 && data[0].id) setIfPresent('ubsId', data[0].id);",
                  "  }",
                  "",
                  "  // 2) Login se n√£o houver token",
                  "  let token = pm.environment.get('token');",
                  "  if (!token) {",
                  "    const email = pm.environment.get('setup_email') || 'joao@teste.com';",
                  "    const senha = pm.environment.get('setup_senha') || 'Senha@123456';",
                  "    const loginResp = await requestJson({",
                  "      url: `${baseUrl}/api/auth/login`,",
                  "      method: 'POST',",
                  "      header: { 'Content-Type': 'application/json' },",
                  "      body: { mode: 'raw', raw: JSON.stringify({ email, senha }) }",
                  "    });",
                  "    if (!loginResp.err && loginResp.res && loginResp.res.code === 200) {",
                  "      const data = safeJson(loginResp.res) || {};",
                  "      setIfPresent('token', data.token);",
                  "      setIfPresent('userId', data.id);",
                  "      setIfPresent('ubsId', data.ubsId || pm.environment.get('ubsId'));",
                  "      token = data.token;",
                  "    }",
                  "  }",
                  "",
                  "  const authHeader = token ? { Authorization: `Bearer ${token}` } : {};",
                  "  const ubsId = pm.environment.get('ubsId');",
                  "  const userId = pm.environment.get('userId');",
                  "",
                  "  // 3) Medicamentos (autenticado)",
                  "  const medResp = await requestJson({ url: `${baseUrl}/api/medicamentos`, method: 'GET', header: authHeader });",
                  "  if (!medResp.err && medResp.res && medResp.res.code === 200) {",
                  "    const data = safeJson(medResp.res);",
                  "    if (Array.isArray(data) && data.length > 0 && data[0].id) setIfPresent('medicamentoId', data[0].id);",
                  "  }",
                  "",
                  "  // 4) Vagas por UBS (AGENTE). Se falhar por role, segue adiante.",
                  "  if (ubsId) {",
                  "    const vagaResp = await requestJson({ url: `${baseUrl}/api/vagas/ubs/${ubsId}`, method: 'GET', header: authHeader });",
                  "    if (!vagaResp.err && vagaResp.res && vagaResp.res.code === 200) {",
                  "      const data = safeJson(vagaResp.res);",
                  "      if (Array.isArray(data) && data.length > 0) {",
                  "        const first = data.find(v => v && v.disponivel) || data[0];",
                  "        if (first && first.id) setIfPresent('vagaId', first.id);",
                  "      }",
                  "    }",
                  "  }",
                  "",
                  "  // 5) Estoques por UBS (AGENTE)",
                  "  if (ubsId) {",
                  "    const estResp = await requestJson({ url: `${baseUrl}/api/estoques/ubs/${ubsId}`, method: 'GET', header: authHeader });",
                  "    if (!estResp.err && estResp.res && estResp.res.code === 200) {",
                  "      const data = safeJson(estResp.res);",
                  "      if (Array.isArray(data) && data.length > 0 && data[0].id) setIfPresent('estoqueId', data[0].id);",
                  "    }",
                  "  }",
                  "",
                  "  // 6) Solicita√ß√µes do usu√°rio (MORADOR)",
                  "  if (userId) {",
                  "    const solResp = await requestJson({ url: `${baseUrl}/api/solicitacoes/usuario/${userId}`, method: 'GET', header: authHeader });",
                  "    if (!solResp.err && solResp.res && solResp.res.code === 200) {",
                  "      const data = safeJson(solResp.res);",
                  "      if (Array.isArray(data) && data.length > 0 && data[0].id) setIfPresent('solicitacaoId', data[0].id);",
                  "    }",
                  "  }",
                  "",
                  "  // 7) Meus agendamentos (MORADOR)",
                  "  if (userId) {",
                  "    const agResp = await requestJson({ url: `${baseUrl}/api/agendamentos/meus?usuarioId=${userId}`, method: 'GET', header: authHeader });",
                  "    if (!agResp.err && agResp.res && agResp.res.code === 200) {",
                  "      const data = safeJson(agResp.res);",
                  "      if (Array.isArray(data) && data.length > 0 && data[0].id) setIfPresent('agendamentoId', data[0].id);",
                  "    }",
                  "  }",
                  "",
                  "  // 8) Not√≠cias por UBS (p√∫blica)",
                  "  if (ubsId) {",
                  "    const notResp = await requestJson({ url: `${baseUrl}/api/noticias/ubs/${ubsId}`, method: 'GET' });",
                  "    if (!notResp.err && notResp.res && notResp.res.code === 200) {",
                  "      const data = safeJson(notResp.res);",
                  "      if (Array.isArray(data) && data.length > 0 && data[0].id) setIfPresent('noticiaId', data[0].id);",
                  "    }",
                  "  }",
                  "",
                  "  pm.test('Setup executado', function () { pm.expect(true).to.eql(true); });",
                  "})();"
                ]
              }
            }
          ],
